{
  "name": "markdown-latex-renderer",
  "displayName": "Markdown LaTeX Renderer",
  "description": "VSCode Extension to convert LaTeX expressions into SVG images on-the-fly on Markdown files",
  "version": "0.0.1",
  "publisher": "agurz",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.renderLaTeX"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.renderLaTeX",
        "title": "Markdown: Render LaTeX expressions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "request-light": "^0.2.0"
  }
}
