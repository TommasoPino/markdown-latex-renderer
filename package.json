{
  "name": "markdown-latex-renderer",
  "displayName": "Markdown LaTeX Renderer",
  "description": "VSCode Extension to convert LaTeX expressions into SVG images on-the-fly on Markdown files",
  "version": "0.0.1",
  "publisher": "agurz",
  "license": "MIT",
  "engines": {
    "vscode": "^1.10.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.renderLaTeX"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.renderLaTeX",
        "title": "Markdown: Render LaTeX expressions"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agurz/Markdown-LaTeX-Renderer.git"
  },
  "devDependencies": {
    "typescript": "^2.2.2",
    "vscode": "^1.0.0",
    "mocha": "^3.2.0",
    "@types/node": "^7.0.12",
    "@types/mocha": "^2.2.40"
  },
  "dependencies": {
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "request-light": "^0.2.0"
  }
}
